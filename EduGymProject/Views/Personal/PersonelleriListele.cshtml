@model IEnumerable<EduGymProject.Models.EntityFramework.Personal>


<div class="container">
    <h2>Personel Listesi</h2>

    <table class="table table-bordered">
        <thead>
            <tr>
                <td>Ad </td>
                <td>Soyad </td>
                <td>Doğum Tarihi </td>
                <td>Maas </td>
            </tr>
        </thead>
        <tbody>
            @foreach (var personel in Model)
            {
                <tr>
                    <td>@personel.Ad</td>
                    <td>@personel.Soyad</td>
                    <td>@personel.DogumTarihi.Value.ToShortDateString()</td>
                    <td>@personel.Maas TL</td>
                </tr>
            }
        </tbody>
    </table>

    </div>